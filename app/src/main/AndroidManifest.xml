<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.knstech.apnaopd">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Volley.VolleySingleton"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Patient.LoginActivity"></activity>
        <activity android:name=".Patient.DoctorViewerActivity" />
        <activity android:name=".Patient.DoctorListActivity" />
        <activity android:name=".Patient.DoctorAppointmentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Patient.HomeActivity"></activity>
        <activity
            android:name=".Patient.MedicineActivity"
            android:parentActivityName=".Patient.HomeActivity" />
        <activity
            android:name=".Retailer.RetailerActivity"
            android:parentActivityName=".Patient.HomeActivity" />
        <activity android:name=".Retailer.ListOfPrescriptionActivity" />
        <activity android:name=".Doctor.DAppointmentViewerActivity"></activity>
        <activity android:name=".Patient.ListOfQuotationsActivity" />
        <activity android:name=".Retailer.RetailerOfferSendActivity" />
        <activity android:name=".Patient.PatientFinalDrugAcceptActivity"></activity>
    </application>

</manifest>